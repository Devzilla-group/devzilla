{
  "printWidth": 80,
  "tabWidth": 2,
  "useTabs": false,
  "semi": true,
  "singleQuote": true,
  "quoteProps": "as-needed",
  "jsxSingleQuote": false,
  "trailingComma": "all",
  "bracketSpacing": true,
  "bracketSameLine": false,
  "arrowParens": "always",
  "endOfLine": "lf",
  "plugins": ["prettier-plugin-tailwindcss"],
  "tailwindCustomOrder": [
    "dark",
    "group-hover",
    "hover",
    "focus",
    "active",
    "disabled",
    "sm",
    "md",
    "lg",
    "xl",
    "2xl"
  ],
  "tailwindFunctions": ["clsx"]
}
